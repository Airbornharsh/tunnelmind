{
  "name": "tunnelmind",
  "version": "0.0.5",
  "description": "Local server for LLM givers",
  "license": "ISC",
  "author": "Harsh Keshri",
  "type": "commonjs",
  "keywords": [
    "llm",
    "tunnelmind",
    "ollama",
    "cli",
    "giver",
    "websocket",
    "ai-tunneling",
    "model-sharing",
    "llm",
    "openai-compatible",
    "airbornharsh"
  ],
  "main": "dist/index.js",
  "bin": {
    "tunnelmind": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:server": "tsx watch src/index.ts server",
    "dev:cli": "tsx watch src/index.ts cli",
    "build": "tsc -b",
    "start": "node dist/index.js",
    "start:server": "node dist/index.js server",
    "start:cli": "node dist/index.js cli",
    "serve": "tsc -b && node dist/index.js",
    "serve:server": "tsc -b && node dist/index.js server",
    "serve:cli": "tsc -b && node dist/index.js cli"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "open": "^10.2.0",
    "openai": "^4.104.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.18.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
